<div class="container" style="margin-bottom: 20px;">
  <div class="row">
    <div class="col-xs-6 col-xs-offset-3">
      <h3 class="text-center"> CatMash </h3>
    </div>
  </div>
  <div class="row">
    <% @cats.each do |cat| %>
      <div class="col-xs-6">
            <div class="avatar" style="background-image: url(<%= cat.url %>);">
            </div>
          <h5 class="text-center"> Score : <%= cat.score %> </h5>
      </div>
    <% end %>

  </div>
</div>